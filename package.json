{
	"name": "markdown-tags",
	"displayName": "markdown-tags",
	"description": "VSCode plugin to handle Markdown tags",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.46.0"
	},
	"publisher": "madeindjs",
	"repository": {
		"type": "Github",
		"url": "https://github.com/madeindjs/vscode-markdown-tags/"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "md-tags.showTags",
				"title": "Show all tags"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"when": "resourceLangId == md",
					"command": "md-tags.showTags",
					"title": "Show all tags",
					"alt": "markdown.showPreviewToSide",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/didyoumean": "^1.2.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.46.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^10.2.0",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"didyoumean": "^1.2.1",
		"gray-matter": "^4.0.2"
	}
}
